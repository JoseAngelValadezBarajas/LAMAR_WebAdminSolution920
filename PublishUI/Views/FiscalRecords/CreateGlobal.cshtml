@model WebAdminUI.Models.FiscalRecords.CashReceiptViewModel

@{
    ViewBag.Title = WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.TitleGlobal;
    var isSubstitution = Model.CancelReasonName == PowerCampus.Entities.Enum.CancelReasonName.ErrorRelacion;
}

@section Styles{
    @Styles.Render("~/Content/datetime")
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "formCreate" }))
{

    <div class="esg-page-header">
        @if (isSubstitution)
        {
            <h1 class="esg-page-header__title" style="height:30px;">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblSubstituteCancelGlobal</h1>
        }
        else
        {
            <h1 class="esg-page-header__title" style="height:30px;">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.TitleGlobal</h1>
        }
    </div>

    <div class="esg-card--panel">
        <div class="esg-card--panel__body">

            @if (isSubstitution)
            {
                <div class="row">
                    <div class="esg-card--panel__body" style="margin-left:20px">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <h3>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblNewInvoice</h3>
                        </div>
                    </div>
                </div>
                <br />

                <div class="row">
                    @*Related CFDI*@
                    <div class="esg-card--panel__body" style="margin-left:20px">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <fieldset>
                                <legend>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblRelatedCFDI</legend>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                        @Html.LabelFor(m => m.RelationTypeDesc, new { @class = "esg-form__label" })<br />
                                        @Html.TextBoxFor(m => m.RelationTypeDesc, Model.RelationTypeDesc, new { @class = "esg-form__input", @id = "RelationTypeDesc", @readonly = "readonly" })<br />
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                        @Html.LabelFor(m => m.CFDIRelated, new { @class = "esg-form__label" })<br />
                                        @Html.TextBoxFor(m => m.CFDIRelated, Model.CFDIRelated, new { @class = "esg-form__input", @id = "CFDIRelated", @readonly = "readonly" })<br />
                                    </div>
                                </div>
                            </fieldset>
                        </div>
                    </div>
                </div>
            }
            <input type="hidden" id="IsSubstitution" value="@isSubstitution.ToString()" />

            @*Receiver*@
            <div class="row">
                <div class="esg-card--panel__body" style="margin-left:20px">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <fieldset>
                            <legend>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.Receiver</legend>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="RecTaxPayerGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.TaxPayerId, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("RecTaxPayerId", PowerCampus.Entities.Constants.GlobalReceiverTaxPayerId, new { @class = "esg-form__input", @id = "RecTaxPayerId", @readonly = "readonly" })
                                            @Html.ValidationMessageFor(m => m.TaxPayerId, "", new { @class = "text-danger validation-label" })
                                            <div id="RecTaxPayerIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="RecNameGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.CorporateName, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBoxFor(m => m.CorporateName, new { @class = "esg-form__input", @id = "RecName", @readonly = "readonly" })
                                            @Html.ValidationMessageFor(m => m.CorporateName, "", new { @class = "text-danger validation-label" })
                                            <label class="text-danger" id="RecNameIncorrect" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.CorporateNameIncorrect</label>
                                            <div id="RecNameIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="EmailGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.preferredReceiverEmail, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBoxFor(m => m.preferredReceiverEmail, new { @class = "esg-form__input", @id = "PreferredEmail", placeholder = @WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.YourEmail })
                                            <label>@Html.ValidationMessageFor(m => m.preferredReceiverEmail, "", new { @class = "text-danger validation-label" })</label>
                                            <div id="EmailIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="RecTaxAddressGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.RecTaxAddress, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBoxFor(m => m.RecTaxAddress, new { @class = "esg-form__input", @id = "RecTaxAddress", @readonly = "readonly" })
                                            <label class="text-danger" id="RecTaxAddressIncorrect" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.RecTaxAddressIncorrect</label>
                                            <div id="FiscalResidencyIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="RecTaxRegimenGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.RecTaxRegimen, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBoxFor(m => m.RecTaxRegimen, new { @class = "esg-form__input", @id = "RecTaxRegimen", @readonly = "readonly" })
                                            <label class="text-danger" id="RecTaxRegimenIncorrect" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.RecTaxRegimenIncorrect</label>
                                            <div id="RecTaxRegimenIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="RecCfdiUsageGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.CfdiUsage, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("CfdiUsage", "S01 - Sin efectos fiscales", new { @class = "esg-form__input", @id = "RecCfdiUsage", @readonly = "readonly" })
                                            <label class="text-danger" id="RecCfdiUsageIncorrect" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.RecCfdiUsageIncorrect</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="RecIssueDateGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssueDate, new { @class = "esg-form__label esg-is-required" })
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("IssueDate", null, new { @class = "esg-form__input", @id = "IssueDate" })
                                            @Html.ValidationMessageFor(m => m.IssueDate, "", new { @class = "text-danger" })
                                            <div class="esg-form__icon esg-icon__container">
                                                <svg class="esg-icon esg-icon--small">
                                                    <use xlink:href="#icon-calendar"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="RecIssueTimeGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssueTime, new { @class = "esg-form__label esg-is-required" })
                                        <div class="esg-form__input-group ">
                                            @Html.TextBox("IssueTime", null, new { @class = "esg-form__input", @id = "IssueTime" })
                                            @Html.ValidationMessageFor(m => m.IssueTime, "", new { @class = "text-danger" })
                                            <div class="esg-form__icon esg-icon__container">
                                                <svg class="esg-icon esg-icon--small">
                                                    <use xlink:href="#icon-clock"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="IssueAlert" class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div class="alert alert-warning" role="alert">
                                        La fecha y Hora de generaci&oacuten esta fuera de rango.
                                    </div>
                                </div>
                        </fieldset>
                    </div>
                </div>
            </div>

            @*Issuer*@
            <div class="row">
                <div class="esg-card--panel__body" style="margin-left:20px;">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <fieldset>
                            <legend>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.Issuer</legend>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="IssTaxPayerGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssTaxpayerId, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("IssTaxPayerId", null, new { @class = "esg-form__input", @id = "IssTaxPayerId" })
                                            <div id="IssTaxPayerDivLookup" class="esg-form__icon esg-icon__container esg-form__icon--submit">
                                                <svg class="esg-icon esg-icon--small">
                                                    <use xlink:href="#icon-lookup"></use>
                                                </svg>
                                            </div>
                                            @Html.ValidationMessage("IssTaxPayerId", "", new { @class = "text-danger validation-label" })
                                            <div id="IssTaxPayerIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    @Html.LabelFor(m => m.IssCorporateName, new { @class = "esg-form__label" })<br />
                                    @Html.TextBox("IssNameCorpName", null, new { @class = "esg-form__input", @id = "IssNameCorpName", @readonly = "readonly" })
                                    <br />
                                    <div id="IssTaxRegimenGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssTaxRegimen, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("IssTaxRegimen", null, new { @class = "esg-form__input", @id = "IssTaxRegimen", @readonly = "readonly" })
                                            @Html.ValidationMessageFor(m => m.IssTaxRegimen, "", new { @class = "text-danger validation-label" })
                                            <div id="IssTaxRegimenIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="IssIssuingAddressGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssIssuingAddress, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.DropDownListFor(m => m.IssIssuingAddress, new SelectList("", "", ""), "", new { @class = "esg-form__input", @id = "IssIssuingAddress" })
                                            @Html.ValidationMessageFor(m => m.IssIssuingAddress, "", new { @class = "text-danger validation-label" })
                                            <div id="IssIssuingAddressIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="IssPlaceIssueGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.IssPlaceIssue, new { @class = "esg-form__label esg-is-required" })
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("IssPlaceIssue", null, new { @class = "esg-form__input", @id = "IssPlaceIssue", @readonly = "readonly" })
                                            @Html.ValidationMessage("IssPlaceIssue", "", new { @class = "text-danger validation-label" })
                                            <div id="IssPlaceIssueIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    @Html.LabelFor(m => m.IssSerial, new { @class = "esg-form__label" })
                                    @Html.DropDownListFor(m => m.IssIssuingAddress, new SelectList("", "", ""), "", new { @class = "esg-form__input", @id = "IssSerial" })
                                    <label id="lblWarnSerial" style="display:none" class="text-danger">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblLastFolioAssigned</label>
                                    <br />
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>

            @*Fiscal Records*@
            <div class="row">
                <div class="esg-card--panel__body" style="margin-left:20px;">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <fieldset>
                            <legend>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.SubHeaderFiscalRecord</legend>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="PaymentTypeGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.PaymentType, new { @class = "esg-form__label" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.DropDownListFor(m => m.PaymentType.Code, new SelectList(Model.PaymentTypeList, "Code", "Description"), new { @class = "esg-form__input", @id = "PaymentType" })
                                            @if (!isSubstitution)
                                            {
                                                <span class="esg-label esg-label--draft" id="NotePaymentType">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.NotePaymentType</span>
                                            }
                                        </div>
                                    </div>
                                    <div id="PaymentMethodGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.ReceiverPaymentMethodDefault, new { @class = "esg-form__label" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBoxFor(m => m.ReceiverPaymentMethodDefault, new { @class = "esg-form__input", @id = "PaymentMethod", @readonly = "readonly" })
                                        </div>
                                    </div>
                                    <div id="CurrencyGroup" class="esg-form__group esg-has-feedback">
                                        <label class="esg-form__label">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.IssCurrency</label><br />
                                        <div class="esg-form__input-group">
                                            @Html.TextBox("IssCurrency", PowerCampus.Entities.Constants.DefaultCurrency, new { @class = "esg-form__input", @id = "IssCurrency", @readonly = "readonly" })
                                        </div>
                                    </div>
                                    <div id="CommentsGroup" class="esg-form__group esg-has-feedback">
                                        <label class="esg-form__label" for="defaultTextarea">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.IssComments</label><br />
                                        <div class="esg-form__input-group">
                                            @Html.TextAreaFor(m => m.Comments, new { @class = "esg-form__input", @id = "IssComments", @rows = "3", maxlength = 500 })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                    <div id="FrequencyGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.FrequencyList, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.DropDownListFor(m => m.Frequency.Code, new SelectList(Model.FrequencyList, "Code", "Description"), @WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblSelect, new { @class = "esg-form__input", @id = "Frequency" })
                                            <label class="text-danger" id="FrequencyRequired" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.FrequencyRequired</label>
                                            <div id="FrequencyIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="MonthGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.MonthList, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.DropDownListFor(m => m.Month.Code, new SelectList(Model.MonthList, "Code", "Description"), @WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblSelect, new { @class = "esg-form__input", @id = "Month" })
                                            <label class="text-danger field-validation-error" id="MonthRequired" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.MonthRequired</label>
                                            <div id="MonthIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                <svg class="esg-icon esg-icon--error">
                                                    <use xlink:href="#icon-error"></use>
                                                </svg>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="YearGroup" class="esg-form__group esg-has-feedback">
                                        @Html.LabelFor(m => m.YearList, new { @class = "esg-form__label esg-is-required" })<br />
                                        <div class="esg-form__input-group">
                                            @Html.DropDownListFor(m => m.Year.Id, new SelectList(Model.YearList, "Code", "Description"), new { @class = "esg-form__input", @id = "Year" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>

            @*Details*@
            <div class="row">
                <div class="esg-card--panel__body" style="margin-left:20px;">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <fieldset>
                            <legend>@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblDetail</legend>
                            <div class="row">
                                <div class="esg-card--panel__body">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        @if (isSubstitution)
                                        {
                                            <div class="row">
                                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                                    <div id="StartDateGroup" class="esg-form__group">
                                                        @Html.LabelFor(m => m.StartDate, new { @class = "esg-form__label esg-is-required" })
                                                        <div class="esg-form__input-group">
                                                            @Html.TextBox("StartDate", null, new { @class = "esg-form__input", @id = "StartDate", @autocomplete = "off" })
                                                            <label class="text-danger" id="StartDateRequired" style="display:none;">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.StartDateRequired</label>
                                                            <label class="text-danger" id="StartDateError" style="display:none;">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblErrorDatesRange</label>
                                                            <div id="StartDateIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                                <svg class="esg-icon esg-icon--error">
                                                                    <use xlink:href="#icon-error"></use>
                                                                </svg>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                                    <div id="EndDateGroup" class="esg-form__group">
                                                        @Html.LabelFor(m => m.EndDate, new { @class = "esg-form__label esg-is-required" })
                                                        <div class="esg-form__input-group ">
                                                            @Html.TextBox("EndDate", null, new { @class = "esg-form__input", @id = "EndDate", @autocomplete = "off" })
                                                            <label class="text-danger" id="EndDateRequired" style="display:none; ">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.EndDateRequired</label>
                                                            <div id="EndDateIcon" class="esg-form__feedback-icon esg-icon__container" style="display:none">
                                                                <svg class="esg-icon esg-icon--error">
                                                                    <use xlink:href="#icon-error"></use>
                                                                </svg>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="esg-collapsible-group esg-collapsible-group--default" role="tablist" aria-multiselectable="true">
                                                <div class="esg-collapsible-group__heading" role="tab">
                                                    <span class="esg-collapsible-group__title">
                                                        <a class="esg-collapsible-group__toggle esg-is-open" id="filtersPanel" role="button" data-toggle="collapse"
                                                           data-parent="#accordion" href="#filtersSection" aria-expanded="true" aria-controls="filtersSection">
                                                            @WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.lblSearchReceipts
                                                            <span class="esg-collapsible-group__icon esg-icon__container" aria-hidden="True">
                                                                <svg class="esg-icon esg-icon--up">
                                                                    <use xlink:href="#icon-arrow"></use>
                                                                </svg>
                                                            </span>
                                                        </a>
                                                    </span>
                                                </div>
                                                <div class="esg-collapsible-group__body collapse in" id="filtersSection" role="tab" aria-expanded="true" aria-labelledby="filtersPanel">
                                                    <div class="row">
                                                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                                            <div id="StartDateGroup" class="esg-form__group">
                                                                @Html.LabelFor(m => m.StartDate, new { @class = "esg-form__label esg-is-required" })
                                                                <div class="esg-form__input-group">
                                                                    @Html.TextBox("StartDate", null, new { @class = "esg-form__input", @id = "StartDate", @autocomplete = "off" })
                                                                    <label class="text-danger" id="StartDateRequired" style="display:none;">@WebAdminUI.Views.FiscalRecords.App_LocalResources.CreateResource.StartDateRequired</label>
