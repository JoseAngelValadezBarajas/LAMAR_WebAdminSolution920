                            <h4><strong>@WebAdminUI.Resources.ElectronicCertificate.Generate.lblControl</strong></h4>
                        </div>
                        <div class="esg-col-xs-6 esg-align-right esg-grid-overlay">
                            <div id="divEditFolio" class="esg-page-header__options">
                                <span class="esg-icon__container">
                                    <svg class="esg-icon esg-icon--small">
                                        <use xlink:href="#icon-edit"></use>
                                    </svg>
                                </span>
                                <a id="btnEditFolio">
                                    <div class="esg-icon__label">
                                        <span>@WebAdminUI.Resources.ElectronicCertificate.Generate.lblEdit</span>
                                    </div>
                                </a>
                            </div>

                            <div id="divSaveCancelFolio" class="esg-page-header__options" style="display: none;">
                                <div style="display:inline-block; margin-right:1rem;">
                                    <a id="btnCancelEditFolio">
                                        <div class="esg-icon__label">
                                            <span>@WebAdminUI.Resources.ElectronicCertificate.Generate.lblCancel</span>
                                        </div>
                                    </a>
                                </div>
                                <div style="display:inline-block">
                                    <span class="esg-icon__container--fill">
                                        <svg class="esg-icon esg-icon--small">
                                            <use xlink:href="#icon-document"></use>
                                        </svg>
                                    </span>
                                    <a id="btnSaveFolio">
                                        <div class="esg-icon__label">
                                            <span>@WebAdminUI.Resources.ElectronicCertificate.Generate.lblSave</span>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="esg-container">
                        <div class="esg-col-xs-12 esg-grid-overlay">
                            <div id="folioFormGroup" class="esg-form__group">
                                <label class="esg-form__label esg-is-required" for="inputFolio">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblFolio</label>
                                <div class="esg-form__input-group">
                                    <input class="esg-form__input" type="text" id="inputFolio" disabled />
                                    <div id="divFolioErrorIcon"></div>
                                </div>
                                <div id="divFolioErrorEmpty" class="esg-form__feedback-text" style="display: none">
                                    @WebAdminUI.Resources.ElectronicCertificate.Generate.lblEnterFolio
                                </div>
                                <div id="divFolioErrorCharacters" class="esg-form__feedback-text" style="display: none">
                                    @WebAdminUI.Resources.ElectronicCertificate.Generate.lblErrorMaxChar
                                </div>
                            </div>
                            <hr />
                        </div>
                    </div>
                </div>
                <div>
                    <div class="esg-container">
