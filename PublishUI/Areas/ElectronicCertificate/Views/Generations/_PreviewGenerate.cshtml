                                <div class="esg-table__container">
                                    <table id="courses_table" class="esg-table">
                                        <thead class="esg-table-head">
                                            <tr class="esg-table-head__row">
                                                <th class="esg-table-head__th" style="width:10%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblCycleRequired</th>
                                                <th class="esg-table-head__th" style="width:10%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblIdRequired</th>
                                                <th class="esg-table-head__th" style="width:25%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblName</th>
                                                <th class="esg-table-head__th" style="width:10%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblGradeRequired</th>
                                                <th class="esg-table-head__th" style="width:20%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblObservations</th>
                                                <th class="esg-table-head__th" style="width:15%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblTypeRequired</th>
                                                <th class="esg-table-head__th" style="width:10%">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblCreditsRequired</th>
                                            </tr>
                                        </thead>
