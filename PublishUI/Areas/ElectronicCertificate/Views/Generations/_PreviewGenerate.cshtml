                    <button class="eds-button eds-button--secondary" id="btnBackIssuingCertificate" type="button">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblBack</button>
                    <button class="eds-button eds-button--primary" id="btnGenerateCertificate" type="button">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblGenerate</button>
                </div>
            </div>
            <div class="esg-container">
                <div id="divGenerateError" style="display:none">
                    <div class="esg-alert esg-alert--error col-xs-12" role="alert">
                        <span class="esg-alert__icon-wrapper">
                            <span class="esg-alert__icon esg-icon__container" aria-hidden="True">
                                <svg class="esg-icon esg-icon--error-dark">
                                    <use xlink:href="#icon-check"></use>
                                </svg>
                            </span>
                        </span>
                        <div class="esg-alert__message">@WebAdminUI.Resources.ElectronicCertificate.Generate.lblErrorLegend</div>
